<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="database.IndentEntity" table="indent" schema="bookstore">
        <id name="indentId">
            <column name="indent_id" sql-type="varchar(255)"/>
            <generator class="increment"/>
        </id>
        <property name="createTime">
            <column name="create_time" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>